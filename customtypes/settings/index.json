{
  "id": "settings",
  "label": "Settings",
  "format": "page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
    "website_name": { "type": "Text", "config": { "label": "Website name", "placeholder": "" } },
    "email": { "type": "Text", "config": { "label": "Email", "placeholder": "" } },
    "socials": {
        "type": "Group",
        "config": {
            "fields": {
                "social": {
                    "type": "Select",
                    "config": {
                        "label": "Social",
                        "placeholder": "",
                        "options": ["Instagram", "Facebook", "Youtube", "Vimeo", "TikTok", "Behance", "LinkedIn"]
                    }
                },
                "link": { "type": "Link", "config": { "label": "Link", "select": null } },
                "label": { "type": "Text", "config": { "label": "Label", "placeholder": "" } }
            },
            "label": "Socials"
        }
    },
      "slices": { "type": "Slices", "fieldset": "Slice Zone", "config": { "choices": {} } }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": { "label": "Meta Description", "placeholder": "A brief summary of the page" },
        "type": "Text"
      },
      "meta_image": {
        "config": { "constraint": { "height": 1260, "width": 2400 }, "label": "Meta Image", "thumbnails": [] },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}
