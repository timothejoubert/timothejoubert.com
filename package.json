{
    "name": "timothe-joubert",
    "version": "1.0.0",
    "author": "Timothé Joubert",
    "description": "Nuxt and Prismic api for graphic & web designer Timothé joubert",
    "engines": {
        "node": "^18"
    },
    "scripts": {
        "dev": "concurrently \"npm:nuxt:dev\" \"npm:slicemachine\" --names \"nuxt,slicemachine\" --prefix-colors green,magenta",
        "nuxt:dev": "nuxt",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "build": "nuxt build",
        "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
        "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
        "lint": "yarn lint:js && yarn lint:style",
        "lint-fix": "yarn lint:js --fix && yarn lint:style --fix",
        "slicemachine": "start-slicemachine"
    },
    "dependencies": {
        "@nuxt/image": "^0.6.2",
        "@nuxtjs/sitemap": "^2.4.0",
        "@nuxtjs/svg": "^0.4.0",
        "core-js": "^3.15.1",
        "nuxt-i18n": "^6.27.2",
        "nuxt": "^2.17.0",
        "throttle-debounce": "^3.0.1"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.7",
        "@nuxt/postcss8": "^1.1.3",
        "@nuxt/types": "^2.15.8",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/eslint-config-typescript": "^6.0.1",
        "@nuxtjs/eslint-module": "^3.1.0",
        "@nuxtjs/prismic": "^1.4.2",
        "@nuxtjs/style-resources": "^1.2.0",
        "@nuxtjs/stylelint-module": "^4.0.0",
        "@prismicio/slice-simulator-vue": "^0.2.3",
        "@prismicio/types": "^0.2.8",
        "@slicemachine/adapter-nuxt2": "^0.3.2",
        "@types/body-scroll-lock": "^3.1.0",
        "@types/node": "^18.11.18",
        "@types/throttle-debounce": "^5.0.0",
        "@types/webpack-env": "^1.18.0",
        "autoprefixer": "^10.4.12",
        "concurrently": "^7.4.0",
        "css-loader": "^4.2.0",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^7.12.1",
        "include-media": "^1.4.10",
        "prettier": "^2.3.2",
        "prismic-ts-codegen": "^0.1.5",
        "sass": "^1.35.1",
        "sass-loader": "^10.1.1",
        "slice-machine-ui": "^1.5.0",
        "stylelint": "^13.13.1",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.19.0",
        "ts-node": "^10.5.0",
        "ts-prismic": "^0.3.0",
        "vue-loader": "15.9.6",
        "vue-typed-mixins": "^0.2.0"
    }
